Page{
  background-color: #f6feff;
}

.containerOne{
  width: 92vw;
  height: 64rpx;
  display: flex;
  flex-direction: row;
  margin-top: 60rpx;
  margin-left: 4vw;
  background-color:#f5f5f5;
}

.headImage{
  width: 8vw;
  height: 8vw;
  padding-top: 5rpx;
  padding-left: 2vw;
  background-color: #f5f5f5;
}

.headText{
  display: flex;
  align-items: center;
  margin-left: 1vw;
  font-size: 32rpx;
  font-weight: 900;
}

.nameHead{
  width: 92vw;
  height: 80rpx;
  margin-top: 56rpx;
  margin-left: 5vw;
  font-size: 32rpx;
  font-weight: 700;
  color:#464d53
}

.nameText{
  width: 100vw;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 8rpx;
  font-size: 36rpx;
  font-weight: 400;
  text-align: center;
  background-color: #f5f5f5;
}

.setting{
  height: 100rpx;
  border-radius: 5vw;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 81rpx;
  font-size: 36rpx;
  font-weight: 600;
  color: #1c386b;
  background-color: #eef5ff;
}

.clock{
  width: 60vw;
  height: 60vw;
  margin-top: 30vw;
  margin-left: 20vw;
  border: #ffffff;
}

.cancel{
  width: 36vw;
  height: 12vw;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 5vw;
  margin-top: 25vw;
  margin-left: 32vw;
  font-size: 36rpx;
  background-color: #d1f8ff;
}