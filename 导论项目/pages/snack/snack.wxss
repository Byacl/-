Page{
  background-color: #f6feff;
}

.snackContainerOne{
  width: 92vw;
  height: 64rpx;
  display: flex;
  flex-direction: row;
  margin-top: 60rpx;
  margin-left: 4vw;
  background-color:#f5f5f5;
}

.snackHeadImage{
  width: 8vw;
  height: 8vw;
  padding-top: 5rpx;
  padding-left: 2vw;
  background-color: #f5f5f5;
}

.snackHeadText{
  display: flex;
  align-items: center;
  margin-left: 1vw;
  font-size: 32rpx;
  font-weight: 900;
}

.snackContainerTwo{
  margin-top: 56rpx;
  height: 80rpx;
  display: flex;
  flex-direction: row;
}

.snackNameImage{
  width: 8vw;
  height: 8vw;
  background-color: #f6feff;
}

.snackNameHead{
  width: 92vw;
  height: 8vw;
  padding-top: 10rpx;
  font-size: 32rpx;
  color:#464d53
}

.snackNameText{
  width: 100vw;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 8rpx;
  font-size: 36rpx;
  font-weight: 400;
  background-color: #f5f5f5;
}

.snackWeightImage{
  width: 8vw;
  height: 8vw;
  background-color: #f6feff;
}

.snackWeightHead{
  width: 92vw;
  height: 8vw;
  padding-top: 10rpx;
  font-size: 32rpx;
  color:#464d53
}

.snackContainerThree{
  display: flex;
  flex-direction: row;
  width: 100vw;
  height: 100rpx;
  background-color: #f5f5f5;
}

.snackWeightBlock{
  width: 40vw;
  height: 100rpx;
  background-color: #f5f5f5;
}

.snackWeightText{
  width: 20vw;
  height: 100rpx;
  display: flex;
  align-items: center;
  text-align:center;
  margin-top: 8rpx;
}

.snackWeightUnit{
  height: 100rpx;
  display: flex;
  align-items: center;
  margin-left: 24vw;
  font-size: 32rpx;
  color: #7f7f80;
}

.snackCalculate{
  height: 100rpx;
  border-radius: 5vw;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 81rpx;
  font-size: 36rpx;
  font-weight: 600;
  color: #1c386b;
  background-color: #eef5ff;
}